<script type="text/javascript" src="~/Views/Home/Research/bootstrap-table.min.js"></script>

<div>
    <table id="datesTable"
           data-toggle="table"
           data-height="600"
           data-pageSize="50"
           data-pageList="[10, 25, 50, 100, 200]"
           data-pagination="true"
           data-search="true"
           data-show-refresh="false"
           data-show-toggle="true"
           data-show-columns="true"
           data-toolbar="#toolbar"
           data-click-to-select="false"
           data-single-select="false">
        <thead>
            <tr>
                <th data-field="Name" data-formatter="nameFormatter">Name</th>
                <th data-field="BirthDate" data-formatter="dateFormatter">Birth</th>
                <th data-field="DeathDate" data-formatter="dateFormatter">Death</th>
                <th data-field="MarriageDate" data-formatter="dateFormatter">Marriage</th>
            </tr>
        </thead>
    </table>
</div>
<div id="getDatesSpinner" style="position: fixed; top: 50%; left: 50%"></div>

<script type="text/javascript" src="~/Views/Home/Research/bootstrap-table.min.js"></script>

<script type="text/javascript">
    $(function () {
        var scriptTag = document.createElement("script");
        scriptTag.src = "/Views/Home/Research/DateProblems/DateProblemsReport.js";
        document.head.appendChild(scriptTag);
    });
</script>

