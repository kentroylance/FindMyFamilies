<form class="form-horizontal" id="feedbackForm" role="form" method="post">
    <div class="left-section">
        <img class="row" style="position: relative; top: 150px;" src="data:image/png;base64," alt="">
    </div>
    <fieldset>
        <legend>Send Us Your Thoughts</legend>

        <div class="right-section">
            <div class="form-group">
                <div class="col-sm-12">
                    <p>If you would like a reply to your feedback, please include your name and email address.</p>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-12">
                    <input name="name" id="feedbackName" placeholder="Name" class="form-control" type="text">
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-12">
                    <div class="radio">
                        <label><input type="radio" id="feedbackBug" name="optradio">Bug</label>&nbsp;&nbsp;
                        <label><input type="radio" id="feedbackFeatureRequest" name="optradio">Feature Request</label>&nbsp;&nbsp;
                        <label><input type="radio" id="feedbackOther" name="optradio">Other</label>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-12">
                    <textarea class="form-control feedback-textarea" id="feedbackMessage" name="message" placeholder="Message"></textarea>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-12">
                    <input name="email" id="feedbackEmail" placeholder="Email Address" class="form-control" type="text">
                </div>
            </div>
        </div>
    </fieldset>
    <div class="col-lg-12">
        <hr />
        <div class="text-center">
            <button id="feedbackSendButton" type="submit" class="btn-u btn-brd btn-brd-hover rounded btn-u-green" data-style="expand-left"><i class="fa fmf-submit24 dialog-buttons"></i> Send Feedback</button>
            <button type='button' id="feedbackClearButton" class="btn-u btn-brd btn-brd-hover rounded btn-u-blue" data-style="expand-left"><i class="fa fmf-clean24 dialog-buttons"></i> Clear</button>
            <button type='button' id="feedbackCloseButton" class="btn-u btn-brd btn-brd-hover rounded btn-u-blue" data-style="expand-left"><i class="fa fmf-close24 dialog-buttons"></i> Close</button>
        </div>
    </div>

</form>

<div id="feedbackSpinner"/>

<script type="text/javascript">require(['main'], function(main) { require(['feedbackController']); });</script>